{
  "name": "proshop-v2",
  "version": "1.0.0",
  "description": "eCommerce application built with the MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "build": "npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SasnarineDeodat/proshop-v2.git"
  },
  "author": "Sasnarine Deodat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SasnarineDeodat/proshop-v2/issues"
  },
  "homepage": "https://github.com/SasnarineDeodat/proshop-v2#readme",
  "dependencies": {
    "@paypal/react-paypal-js": "^8.1.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.3.4",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
